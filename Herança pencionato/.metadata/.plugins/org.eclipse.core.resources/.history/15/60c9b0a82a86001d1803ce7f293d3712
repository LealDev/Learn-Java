package entities;

public class BusinessAccount extends Account{
	
	private double loanLimit;

	public BusinessAccount(double loanLimit) {
		super();
		this.loanLimit = loanLimit;
	}

	public double getLoanLimit() {
		return loanLimit;
	}

	public void setLoanLimit(double loanLimit) {
		this.loanLimit = loanLimit;
	}
	

}
